{
  "name": "anzenox",
  "version": "1.0.0",
  "description": "A simple language written in JavaScript using NodeJS",
  "main": "dist/anzenox.js",
  "type": "module",
  "bin": {
    "azc": "dist/anzenox.js"
  },
  "scripts": {
    "dev": "npm run ts && npm run bundle",
    "bundle": "node bundle.cjs",
    "ts": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "npx mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xFarish/Anzenox.git"
  },
  "keywords": [
    "language"
  ],
  "author": "Farish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xFarish/Anzenox/issues"
  },
  "homepage": "https://github.com/xFarish/Anzenox#readme",
  "dependencies": {
    "binaryen": "^101.0.0",
    "command-line-args": "^5.1.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/node": "^15.12.5",
    "esbuild": "^0.12.12",
    "mocha": "^9.0.1",
    "typescript": "^4.3.4"
  }
}
